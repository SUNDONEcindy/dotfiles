{
    "python.pythonPath": "ipython",
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true,
        "suppressWelcomeNotice": true
    },
    "python.formatting.provider": "black",
    // Enable crash reports to be sent to Microsoft.
    // This option requires restart to take effect.
    "telemetry.enableCrashReporter": false,
    // Enable usage data and errors to be sent to Microsoft.
    "telemetry.enableTelemetry": false,
    "gitlens.keymap": "chorded",
    "gitlens.mode.active": "zen",
    "workbench.iconTheme": "vscode-icons",
    "sync.gist": "ccacf01b7ae269de00170aec838d2658",
    "sync.quietSync": false,
    "sync.removeExtensions": true,
    "sync.syncExtensions": true,
    "sync.autoDownload": false,
    "sync.autoUpload": false,
    "sync.forceDownload": false,
    "editor.minimap.enabled": false,
    "vim.easymotion": true,
    "workbench.commandPalette.preserveInput": true,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.labelFormat": "default",
    "editor.lineNumbers": "off",
    "editor.find.autoFindInSelection": true,
    "editor.renderLineHighlight": "none",
    "editor.renderWhitespace": "all",
    "workbench.editor.tabSizing": "shrink",
    "workbench.quickOpen.preserveInput": true,
    "workbench.statusBar.feedback.visible": false,
    "window.title": "${dirty}${activeEditorLong}${separator}${rootName}",
    "search.smartCase": true,
    "debug.inlineValues": true,
    "terminal.external.osxExec": "iTerm2.app",
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.shell.osx": "/usr/local/bin/zsh",
    "python.linting.flake8Enabled": true,
    "ai.azureml.enable": false,
    "ai.telemetry.enable": false,
    "path-intellisense.autoSlashAfterDirectory": true,
    "vim.sneak": true,
    "vim.statusBarColorControl": false,
    "workbench.colorCustomizations": {
        "statusBar.background": "#005f5f",
        "statusBar.noFolderBackground": "#005f5f",
        "statusBar.debuggingBackground": "#005f5f"
    },
    "vim.useSystemClipboard": true,
    "vim.visualstar": true,
    "python.linting.flake8Args": [
        "--max-line-length=120",
        "--ignore=W293,E501,E203,E402,F841,F401,E302,E305",
    ],
    "python.linting.mypyEnabled": false,
    "python.linting.pydocstyleEnabled": false,
    "breadcrumbs.enabled": true,
    "editor.wordWrap": "on",
    "todohighlight.include": [
        "**/*.js",
        "**/*.jsx",
        "**/*.ts",
        "**/*.tsx",
        "**/*.html",
        "**/*.php",
        "**/*.css",
        "**/*.scss",
        "**/*.py"
    ],
    "ai.homepage.openOnStartup": false,
    "gitlens.views.fileHistory.enabled": true,
    "gitlens.views.lineHistory.enabled": true,
    "editor.fontSize": 13,
    "git.confirmSync": false,
    "autoDocstring.docstringFormat": "sphinx",
    "path-intellisense.absolutePathToWorkspace": false,
    "vim.foldfix": true,
    "vim.highlightedyank.enable": true,
    "vim.searchHighlightColor": "rgba(250, 240, 170, 0.5)",
    "editor.renderControlCharacters": false,
    "[python]": {
        "editor.defaultFormatter": "ms-python.python"
    },
    "editor.formatOnSave": true,
    "python.dataScience.askForKernelRestart": false,
    "git.fetchOnPull": true,
    "git.autoStash": true,
    "python.testing.autoTestDiscoverOnSaveEnabled": false,
    "workbench.colorTheme": "Dark (Molokai)",
    "latex-workshop.view.pdf.viewer": "tab",
    "terminal.integrated.inheritEnv": false,
    "vscode-neovim.neovimPath": "/usr/local/bin/nvim",
    "todo-tree.tree.showScanModeButton": false,
    // Vim easymotion is required for Jump menu - <SPC> j
    "vim.easymotion": true,
    // Use system clipboard for vim
    "vim.useSystemClipboard": true,
    // Trigger the main which key menu with in the active editor with vim
    // This cannot be put into keybindings.json because it will
    // create conflict with Vim.
    // https://github.com/stevenguh/spacecode/issues/3
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<space>"
            ],
            "commands": [
                "vspacecode.space"
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<space>"
            ],
            "commands": [
                "vspacecode.space"
            ]
        }
    ],
    "python.dataScience.sendSelectionToInteractiveWindow": true
}